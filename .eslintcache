target.value\n    setPie({\n      ...pie,\n      pieces: p\n    })\n  }\n\n  String.prototype.isEmpty = function() {\n    return (this.length === 0 || !this.trim())\n  }\n\n  return (\n    <div>\n      <h1>Create Pie</h1>\n      <form className='pie-form'>\n        <label htmlFor='title'>Title</label>\n        <input type='text' name='title' value={ pie.title } onChange={ setTitle } />\n\n        <label htmlFor='piece00'>Piece</label>\n        <input type='text' name='piece00' id={0} value={ pie.piece00 } onChange={ setPiece } />\n\n        <label htmlFor='piece01'>Piece</label>\n        <input type='text' name='piece01' id={1} value={ pie.piece01 } onChange={ setPiece }/>\n\n        <label htmlFor='piece02'>Piece</label>\n        <input type='text' name='piece02' id={2} value={ pie.piece02 } onChange={ setPiece }/>\n\n        <label htmlFor='piece03'>Piece</label>\n        <input type='text' name='piece03' id={3} value={ pie.piece03 } onChange={ setPiece }/>\n\n        <label htmlFor='piece04'>Piece</label>\n        <input type='text' name='piece04' id={4} value={ pie.piece04 } onChange={ setPiece }/>\n\n        <label htmlFor='piece05'>Piece</label>\n        <input type='text' name='piece05' id={5} value={ pie.piece05 } onChange={ setPiece }/>\n        \n        <label htmlFor='piece06'>Piece</label>\n        <input type='text' name='piece06' id={6} value={ pie.piece06 } onChange={ setPiece }/>\n\n        <label htmlFor='piece07'>Piece</label>\n        <input type='text' name='piece07' id={7} value={ pie.piece07 } onChange={ setPiece }/>\n\n        <input type='submit' value=\"Save\" onClick={(event) => {\n          event.preventDefault()\n          if(pie.title.isEmpty() || pie.pieces[0].isEmpty() || pie.pieces[1].isEmpty() || pie.pieces[2].isEmpty() || pie.pieces[3].isEmpty() || pie.pieces[4].isEmpty() || pie.pieces[5].isEmpty() || pie.pieces[6].isEmpty() || pie.pieces[7].isEmpty()) {\n            alert('All fields are required')\n          }\n          else {\n            createPie(pie, history)\n          }\n        }} />\n      </form>\n    </div>\n  )\n}\n\nPieForm.propTypes = {\n  createPie: PropTypes.func.isRequired\n}\n\nexport default connect(null, { createPie })(PieForm)\n","/home/geodevadmin/programming/node_apps/pies_fe/src/components/PieGalleryContainer.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/PieGallery.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/Navigation.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/SignupForm.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/LoginForm.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/actions/userActions.js",["97"],"import { SIGNUP_REQUEST, SIGNUP_SUCCESS, SIGNUP_FAILURE, LOGIN_REQUEST, LOGIN_SUCCESS, LOGIN_FAILURE, CURRENT_USER_REFRESH_REQUEST, CURRENT_USER_REFRESH_FAILURE, CURRENT_USER_REFRESH_SUCCESS, LOGOUT } from './types'\nimport { URL_PREFIX } from './urlPrefix'\nimport getErrorMessage from '../_helpers/errorHelper'\n\n//TODO, where should this go?\nString.prototype.capitalize = function() {\n  return this.charAt(0).toUpperCase() + this.slice(1)\n}\n\nexport const login = (email, password, history) => {\n  \n  return dispatch => {\n\n    dispatch({ type: LOGIN_REQUEST, email })\n\n    const requestOptions = {\n      method: 'POST',\n      headers: { 'Content-Type': 'application/json' },\n      body: JSON.stringify({\"user\":{\"email\":email, \"password\":password}})\n    }\n\n    fetch(`${ URL_PREFIX }/api/users/login`, requestOptions)\n      .then(response => response.json())\n      .then(response => {\n        \n        if(response.error || response.errors) {\n          dispatch({ \n            type: LOGIN_FAILURE,\n            error: getErrorMessage(response)\n          })\n        }\n        else {\n          dispatch({\n            type: LOGIN_SUCCESS,\n            payload: response.user\n          })\n          history.push('/pies', null)\n        }\n      })\n      .catch((error) => {\n        dispatch({ \n          type: LOGIN_FAILURE,\n          error: error.toString()\n        })\n      })\n    }\n}\n\nexport const logout = () => {\n  return { type: LOGOUT }\n}\n\nexport const signup = (email, password, firstname, lastname, history) => {\n  \n  return dispatch => {\n\n    dispatch({ type: SIGNUP_REQUEST, email })\n\n    const requestOptions = {\n      method: 'POST',\n      headers: { 'Content-Type': 'application/json' },\n      body: JSON.stringify({\"user\":{\"email\":email, \"password\":password, \"first_name\": firstname, \"last_name\": lastname}})\n    }\n\n    fetch(`${ URL_PREFIX }/api/users`, requestOptions)\n      .then(response => response.json())\n      .then(response => {\n        if(response.error || response.errors) {\n          dispatch({ \n            type: SIGNUP_FAILURE,\n            error: getErrorMessage(response)\n          })\n        }\n        else {\n          //only pick up id, email, firstName, lastName, and token for user\n          dispatch({\n            type: SIGNUP_SUCCESS,\n            payload: response.user\n          })\n          history.push('/pies', null)\n        }\n      })\n      .catch((error) => {\n        dispatch({ \n          type: SIGNUP_FAILURE,\n          error: error.toString()\n        })\n      })\n    }\n}\n\nexport const refreshUser = (user) => {\n\n  return dispatch => {\n\n    dispatch({ type: CURRENT_USER_REFRESH_REQUEST, email: user.email })\n\n    const requestOptions = {\n      method: 'GET',\n      headers: (user && user.token) ? { 'Authorization': 'Bearer ' + user.token } : {}\n    }\n\n    fetch(`${ URL_PREFIX }/user`, requestOptions)\n      .then(response => response.json())\n      .then(response => {\n        if(response.error || response.errors) {\n          dispatch({ \n            type: CURRENT_USER_REFRESH_FAILURE,\n            error: getErrorMessage(response)\n          })\n        }\n        else {\n          dispatch({\n            type: CURRENT_USER_REFRESH_SUCCESS,\n            payload: response.user\n          })\n        }\n      })\n      .catch((error) => {\n        dispatch({ \n          type: CURRENT_USER_REFRESH_FAILURE,\n          error: error.toString()\n        })\n      })\n    }\n}\n","/home/geodevadmin/programming/node_apps/pies_fe/src/_helpers/errorHelper.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/reducers/authenticationReducer.js",["98"],"import { SIGNUP_REQUEST, SIGNUP_SUCCESS, SIGNUP_FAILURE, LOGIN_REQUEST, LOGIN_SUCCESS, LOGIN_FAILURE, CURRENT_USER_REFRESH_REQUEST, CURRENT_USER_REFRESH_FAILURE, CURRENT_USER_REFRESH_SUCCESS, LOGOUT } from '../actions/types';\n\nconst initialState = {\n  loggedIn: false,\n  loading: false,\n  user: {}\n}\n\nexport default (state = initialState, action) => {\n  switch (action.type) {\n    case LOGIN_REQUEST:\n      return {\n        ...state,\n        loading: true\n      }\n    case LOGIN_SUCCESS:\n      return {\n        loggedIn: true,\n        loading: false,\n        user: action.payload\n      }\n    case LOGIN_FAILURE:\n      return {\n        loggedIn: false,\n        loading: false,\n        errorMessage: action.error\n      }\n    case LOGOUT:\n      return {\n        loggedIn: false,\n        loading: false,\n        user: {}\n      }\n    case SIGNUP_REQUEST:\n      return {\n        ...state,\n        loggedIn: true,\n      }\n    case SIGNUP_SUCCESS:\n      return {\n        loggedIn: true,\n        loading: false,\n        user: action.payload\n      }\n    case SIGNUP_FAILURE:\n      return {\n        loading: false,\n        loggedIn: false,\n        errorMessage: action.error\n      }\n    case CURRENT_USER_REFRESH_REQUEST:\n      return {\n        ...state,\n        loading: false\n      }\n    case CURRENT_USER_REFRESH_SUCCESS:\n      return {\n        loggedIn: true,\n        loading: false,\n        user: action.payload\n      }\n    case CURRENT_USER_REFRESH_FAILURE:\n      return {\n        loggedIn: false,\n        loading: false,\n        errorMessage: action.error\n      }\n    default:\n      return state\n  }\n}",["99","100"],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/PieController.js",["101"],"import React, { useEffect } from 'react'\nimport PropTypes from 'prop-types'\nimport Pie from './Pie'\nimport PieControls from './PieControls'\nimport { fetchPie } from '../actions/pieActions'\nimport { useSelector, useDispatch} from 'react-redux'\n\nconst PieController = ({\n  id = 0\n}) => {\n\n  const getPie = () => Object.keys(pie).length > 0 ? <Pie pieData={ pie } /> : null\n  const getPieControls = () => Object.keys(pie).length > 0 ? <PieControls pieData={ pie } /> : null\n  const pie = useSelector(state => state.pieReducer.pie)\n  const dispatch = useDispatch()\n\n  useEffect(() => {\n    dispatch(fetchPie(id))\n  }, [id])\n  \n  return (\n    <div className='pie-container'>\n      { getPie() }\n      { getPieControls() }\n    </div>\n  )\n}\n\nPieController.propTypes = {\n  id: PropTypes.number.isRequired\n}\n\nexport default PieController","/home/geodevadmin/programming/node_apps/pies_fe/src/components/PieControls.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/PrivateRoute.js",[],{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","severity":1,"message":"107","line":7,"column":1,"nodeType":"108","endLine":27,"endColumn":2},{"ruleId":"109","severity":1,"message":"110","line":33,"column":3,"nodeType":"111","messageId":"112","endLine":35,"endColumn":4},{"ruleId":"109","severity":1,"message":"110","line":6,"column":1,"nodeType":"111","messageId":"112","endLine":8,"endColumn":2},{"ruleId":"106","severity":1,"message":"107","line":9,"column":1,"nodeType":"108","endLine":71,"endColumn":2},{"ruleId":"102","replacedBy":"113"},{"ruleId":"104","replacedBy":"114"},{"ruleId":"115","severity":1,"message":"116","line":19,"column":6,"nodeType":"117","endLine":19,"endColumn":10,"suggestions":"118"},"n