const p = [...pie.pieces]\n    p[event.target.id] = event.target.value\n    setPie({\n      ...pie,\n      pieces: p\n    })\n  }\n\n  //TODO: where to put this?\n  String.prototype.isEmpty = function() {\n    return (this.length === 0 || !this.trim())\n  }\n\n  const savePie = (event) => {\n    event.preventDefault()\n    if(pie.title.isEmpty() || pie.pieces[0].isEmpty() || pie.pieces[1].isEmpty() || pie.pieces[2].isEmpty() || pie.pieces[3].isEmpty() || pie.pieces[4].isEmpty() || pie.pieces[5].isEmpty() || pie.pieces[6].isEmpty() || pie.pieces[7].isEmpty()) {\n      alert('All fields are required')\n    }\n    else {\n      pie.user_id = user.id\n      dispatch(createPie(pie))\n      history.push('/pies')\n    }\n  }\n\n  return (\n    <div>\n      <h1>New Pie</h1>\n      <form className='form'>\n        <label htmlFor='title'>Title</label>\n        <input type='text' name='title' value={ pie.title } onChange={ setTitle } />\n\n        <label htmlFor='piece00'>Piece</label>\n        <input type='text' name='piece00' id={0} defaultValue={ pie.pieces[0] } onChange={ setPiece } />\n\n        <label htmlFor='piece01'>Piece</label>\n        <input type='text' name='piece01' id={1} defaultValue={ pie.pieces[1] } onChange={ setPiece }/>\n\n        <label htmlFor='piece02'>Piece</label>\n        <input type='text' name='piece02' id={2} defaultValue={ pie.pieces[2] } onChange={ setPiece }/>\n\n        <label htmlFor='piece03'>Piece</label>\n        <input type='text' name='piece03' id={3} defaultValue={ pie.pieces[3] } onChange={ setPiece }/>\n\n        <label htmlFor='piece04'>Piece</label>\n        <input type='text' name='piece04' id={4} defaultValue={ pie.pieces[4] } onChange={ setPiece }/>\n\n        <label htmlFor='piece05'>Piece</label>\n        <input type='text' name='piece05' id={5} defaultValue={ pie.pieces[5] } onChange={ setPiece }/>\n        \n        <label htmlFor='piece06'>Piece</label>\n        <input type='text' name='piece06' id={6} defaultValue={ pie.pieces[6] } onChange={ setPiece }/>\n\n        <label htmlFor='piece07'>Piece</label>\n        <input type='text' name='piece07' id={7} defaultValue={ pie.pieces[7] } onChange={ setPiece }/>\n\n        <input type='submit' value=\"Save\" onClick={ savePie } />\n      </form>\n    </div>\n  )\n}\n\nexport default PieForm\n","/home/geodevadmin/programming/node_apps/pies_fe/src/components/PieGalleryContainer.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/PieGallery.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/Navigation.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/SignupForm.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/LoginForm.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/actions/userActions.js",["100"],"import { SIGNUP_REQUEST, SIGNUP_SUCCESS, SIGNUP_FAILURE, LOGIN_REQUEST, LOGIN_SUCCESS, LOGIN_FAILURE, CURRENT_USER_REFRESH_REQUEST, CURRENT_USER_REFRESH_FAILURE, CURRENT_USER_REFRESH_SUCCESS, LOGOUT } from './types'\nimport { URL_PREFIX } from './urlPrefix'\nimport getErrorMessage from '../_helpers/errorHelper'\n\n//TODO, where should this go?\nString.prototype.capitalize = function() {\n  return this.charAt(0).toUpperCase() + this.slice(1)\n}\n\nexport const login = (email, password, history) => {\n  \n  return dispatch => {\n\n    dispatch({ type: LOGIN_REQUEST, email })\n\n    const requestOptions = {\n      method: 'POST',\n      headers: { 'Content-Type': 'application/json' },\n      body: JSON.stringify({\"user\":{\"email\":email, \"password\":password}})\n    }\n\n    fetch(`${ URL_PREFIX }/api/users/login`, requestOptions)\n      .then(response => response.json())\n      .then(response => {\n        \n        if(response.error || response.errors) {\n          dispatch({ \n            type: LOGIN_FAILURE,\n            error: getErrorMessage(response)\n          })\n        }\n        else {\n          return dispatch({\n            type: LOGIN_SUCCESS,\n            payload: response.user\n          })\n        }\n      })\n      .then(() => {\n        history.push('/pies', null)\n      })\n      .catch((error) => {\n        dispatch({ \n          type: LOGIN_FAILURE,\n          error: error.toString()\n        })\n      })\n    }\n}\n\nexport const logout = () => {\n  return { type: LOGOUT }\n}\n\nexport const signup = (email, password, firstname, lastname, history) => {\n  \n  return dispatch => {\n\n    dispatch({ type: SIGNUP_REQUEST, email })\n\n    const requestOptions = {\n      method: 'POST',\n      headers: { 'Content-Type': 'application/json' },\n      body: JSON.stringify({\"user\":{\"email\":email, \"password\":password, \"first_name\": firstname, \"last_name\": lastname}})\n    }\n\n    fetch(`${ URL_PREFIX }/api/users`, requestOptions)\n      .then(response => response.json())\n      .then(response => {\n        if(response.error || response.errors) {\n          dispatch({ \n            type: SIGNUP_FAILURE,\n            error: getErrorMessage(response)\n          })\n        }\n        else {\n          //only pick up id, email, firstName, lastName, and token for user\n          dispatch({\n            type: SIGNUP_SUCCESS,\n            payload: response.user\n          })\n          history.push('/pies', null)\n        }\n      })\n      .catch((error) => {\n        dispatch({ \n          type: SIGNUP_FAILURE,\n          error: error.toString()\n        })\n      })\n    }\n}\n\nexport const refreshUser = (user) => {\n\n  return dispatch => {\n\n    dispatch({ type: CURRENT_USER_REFRESH_REQUEST, email: user.email })\n\n    const requestOptions = {\n      method: 'GET',\n      headers: (user && user.token) ? { 'Authorization': 'Bearer ' + user.token } : {}\n    }\n\n    fetch(`${ URL_PREFIX }/user`, requestOptions)\n      .then(response => response.json())\n      .then(response => {\n        if(response.error || response.errors) {\n          dispatch({ \n            type: CURRENT_USER_REFRESH_FAILURE,\n            error: getErrorMessage(response)\n          })\n        }\n        else {\n          dispatch({\n            type: CURRENT_USER_REFRESH_SUCCESS,\n            payload: response.user\n          })\n        }\n      })\n      .catch((error) => {\n        dispatch({ \n          type: CURRENT_USER_REFRESH_FAILURE,\n          error: error.toString()\n        })\n      })\n    }\n}\n","/home/geodevadmin/programming/node_apps/pies_fe/src/_helpers/errorHelper.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/reducers/authenticationReducer.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/PieController.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/PieControls.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/PrivateRoute.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/store.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/ProfileContainer.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/Profile.js",[],{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","severity":1,"message":"106","line":34,"column":3,"nodeType":"107","messageId":"108","endLine":36,"endColumn":4},{"ruleId":"105","severity":1,"message":"106","line":6,"column":1,"nodeType":"107","messageId":"108","endLine":8,"endColumn":2},"no-native-reassign",["109"],"no-negated-in-lhs",["110"],"no-extend-native","String prototype is read only, properties should not be added.","AssignmentExpression","unexpected","no-global-assign","no-unsafe-negation"]