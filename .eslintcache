in/programming/node_apps/pies_fe/src/index.js",[],["90","91"],"/home/geodevadmin/programming/node_apps/pies_fe/src/App.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/Pie.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/actions/pieActions.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/actions/types.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/actions/urlPrefix.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/reducers/index.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/reducers/pieReducer.js",["92"],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/PieForm.js",["93"],"import React, { useState } from \"react\"\nimport { createPie } from '../actions/pieActions'\nimport { useHistory } from \"react-router\"\nimport { useSelector, useDispatch } from 'react-redux'\n\nexport const PieForm = () => {\n\n  const user = useSelector(state => state.authenticationReducer.user)\n  const dispatch = useDispatch()\n  const history = useHistory()\n\n  const [pie, setPie] = useState({\n    title: '',\n    pieces: ['Goal 1', 'Goal 2', 'Goal 3', 'Goal 4', 'Goal 5', 'Goal 6', 'Goal 7', 'Goal 8']\n  })\n\n  const setTitle = (event) => {\n    setPie({\n      ...pie,\n      title: event.target.value\n    })\n  }\n\n  const setPiece = (event) => {\n    const p = [...pie.pieces]\n    p[event.target.id] = event.target.value\n    setPie({\n      ...pie,\n      pieces: p\n    })\n  }\n\n  //TODO: where to put this?\n  String.prototype.isEmpty = function() {\n    return (this.length === 0 || !this.trim())\n  }\n\n  return (\n    <div>\n      <h1>Create Pie</h1>\n      <form className='form'>\n        <label htmlFor='title'>Title</label>\n        <input type='text' name='title' value={ pie.title } onChange={ setTitle } />\n\n        <label htmlFor='piece00'>Piece</label>\n        <input type='text' name='piece00' id={0} defaultValue={ pie.pieces[0] } onChange={ setPiece } />\n\n        <label htmlFor='piece01'>Piece</label>\n        <input type='text' name='piece01' id={1} defaultValue={ pie.pieces[1] } onChange={ setPiece }/>\n\n        <label htmlFor='piece02'>Piece</label>\n        <input type='text' name='piece02' id={2} defaultValue={ pie.pieces[2] } onChange={ setPiece }/>\n\n        <label htmlFor='piece03'>Piece</label>\n        <input type='text' name='piece03' id={3} defaultValue={ pie.pieces[3] } onChange={ setPiece }/>\n\n        <label htmlFor='piece04'>Piece</label>\n        <input type='text' name='piece04' id={4} defaultValue={ pie.pieces[4] } onChange={ setPiece }/>\n\n        <label htmlFor='piece05'>Piece</label>\n        <input type='text' name='piece05' id={5} defaultValue={ pie.pieces[5] } onChange={ setPiece }/>\n        \n        <label htmlFor='piece06'>Piece</label>\n        <input type='text' name='piece06' id={6} defaultValue={ pie.pieces[6] } onChange={ setPiece }/>\n\n        <label htmlFor='piece07'>Piece</label>\n        <input type='text' name='piece07' id={7} defaultValue={ pie.pieces[7] } onChange={ setPiece }/>\n\n        <input type='submit' value=\"Save\" onClick={(event) => {\n          event.preventDefault()\n          if(pie.title.isEmpty() || pie.pieces[0].isEmpty() || pie.pieces[1].isEmpty() || pie.pieces[2].isEmpty() || pie.pieces[3].isEmpty() || pie.pieces[4].isEmpty() || pie.pieces[5].isEmpty() || pie.pieces[6].isEmpty() || pie.pieces[7].isEmpty()) {\n            alert('All fields are required')\n          }\n          else {\n            pie.user_id = user.id\n            dispatch(createPie(pie, user, history))\n          }\n        }} />\n      </form>\n    </div>\n  )\n}\n\nexport default PieForm\n","/home/geodevadmin/programming/node_apps/pies_fe/src/components/PieGalleryContainer.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/PieGallery.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/Navigation.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/SignupForm.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/LoginForm.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/actions/userActions.js",["94"],"import { SIGNUP_REQUEST, SIGNUP_SUCCESS, SIGNUP_FAILURE, LOGIN_REQUEST, LOGIN_SUCCESS, LOGIN_FAILURE, CURRENT_USER_REFRESH_REQUEST, CURRENT_USER_REFRESH_FAILURE, CURRENT_USER_REFRESH_SUCCESS, LOGOUT } from './types'\nimport { URL_PREFIX } from './urlPrefix'\nimport getErrorMessage from '../_helpers/errorHelper'\n\n//TODO, where should this go?\nString.prototype.capitalize = function() {\n  return this.charAt(0).toUpperCase() + this.slice(1)\n}\n\nexport const login = (email, password, history) => {\n  \n  return dispatch => {\n\n    dispatch({ type: LOGIN_REQUEST, email })\n\n    const requestOptions = {\n      method: 'POST',\n      headers: { 'Content-Type': 'application/json' },\n      body: JSON.stringify({\"user\":{\"email\":email, \"password\":password}})\n    }\n\n    fetch(`${ URL_PREFIX }/api/users/login`, requestOptions)\n      .then(response => response.json())\n      .then(response => {\n        \n        if(response.error || response.errors) {\n          dispatch({ \n            type: LOGIN_FAILURE,\n            error: getErrorMessage(response)\n          })\n        }\n        else {\n          dispatch({\n            type: LOGIN_SUCCESS,\n            payload: response.user\n          })\n          history.push('/pies', null)\n        }\n      })\n      .catch((error) => {\n        dispatch({ \n          type: LOGIN_FAILURE,\n          error: error.toString()\n        })\n      })\n    }\n}\n\nexport const logout = () => {\n  return { type: LOGOUT }\n}\n\nexport const signup = (email, password, firstname, lastname, history) => {\n  \n  return dispatch => {\n\n    dispatch({ type: SIGNUP_REQUEST, email })\n\n    const requestOptions = {\n      method: 'POST',\n      headers: { 'Content-Type': 'application/json' },\n      body: JSON.stringify({\"user\":{\"email\":email, \"password\":password, \"first_name\": firstname, \"last_name\": lastname}})\n    }\n\n    fetch(`${ URL_PREFIX }/api/users`, requestOptions)\n      .then(response => response.json())\n      .then(response => {\n        if(response.error || response.errors) {\n          dispatch({ \n            type: SIGNUP_FAILURE,\n            error: getErrorMessage(response)\n          })\n        }\n        else {\n          //only pick up id, email, firstName, lastName, and token for user\n          dispatch({\n            type: SIGNUP_SUCCESS,\n            payload: response.user\n          })\n          history.push('/pies', null)\n        }\n      })\n      .catch((error) => {\n        dispatch({ \n          type: SIGNUP_FAILURE,\n          error: error.toString()\n        })\n      })\n    }\n}\n\nexport const refreshUser = (user) => {\n\n  return dispatch => {\n\n    dispatch({ type: CURRENT_USER_REFRESH_REQUEST, email: user.email })\n\n    const requestOptions = {\n      method: 'GET',\n      headers: (user && user.token) ? { 'Authorization': 'Bearer ' + user.token } : {}\n    }\n\n    fetch(`${ URL_PREFIX }/user`, requestOptions)\n      .then(response => response.json())\n      .then(response => {\n        if(response.error || response.errors) {\n          dispatch({ \n            type: CURRENT_USER_REFRESH_FAILURE,\n            error: getErrorMessage(response)\n          })\n        }\n        else {\n          dispatch({\n            type: CURRENT_USER_REFRESH_SUCCESS,\n            payload: response.user\n          })\n        }\n      })\n      .catch((error) => {\n        dispatch({ \n          type: CURRENT_USER_REFRESH_FAILURE,\n          error: error.toString()\n        })\n      })\n    }\n}\n","/home/geodevadmin/programming/node_apps/pies_fe/src/_helpers/errorHelper.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/reducers/authenticationReducer.js",["95"],"import { SIGNUP_REQUEST, SIGNUP_SUCCESS, SIGNUP_FAILURE, LOGIN_REQUEST, LOGIN_SUCCESS, LOGIN_FAILURE, CURRENT_USER_REFRESH_REQUEST, CURRENT_USER_REFRESH_FAILURE, CURRENT_USER_REFRESH_SUCCESS, LOGOUT } from '../actions/types';\n\nconst initialState = {\n  loggedIn: false,\n  loading: false,\n  user: {}\n}\n\nexport default (state = initialState, action) => {\n  switch (action.type) {\n    case LOGIN_REQUEST:\n      return {\n        ...state,\n        loading: true\n      }\n    case LOGIN_SUCCESS:\n      return {\n        loggedIn: true,\n        loading: false,\n        user: action.payload\n      }\n    case LOGIN_FAILURE:\n      return {\n        loggedIn: false,\n        loading: false,\n        errorMessage: action.error\n      }\n    case LOGOUT:\n      return {\n        loggedIn: false,\n        loading: false,\n        user: {}\n      }\n    case SIGNUP_REQUEST:\n      return {\n        ...state,\n        loggedIn: true,\n      }\n    case SIGNUP_SUCCESS:\n      return {\n        loggedIn: true,\n        loading: false,\n        user: action.payload\n      }\n    case SIGNUP_FAILURE:\n      return {\n        loading: false,\n        loggedIn: false,\n        errorMessage: action.error\n      }\n    case CURRENT_USER_REFRESH_REQUEST:\n      return {\n        ...state,\n        loading: false\n      }\n    case CURRENT_USER_REFRESH_SUCCESS:\n      return {\n        loggedIn: true,\n        loading: false,\n        user: action.payload\n      }\n    case CURRENT_USER_REFRESH_FAILURE:\n      return {\n        loggedIn: false,\n        loading: false,\n        errorMessage: action.error\n      }\n    default:\n      return state\n  }\n}","/home/geodevadmin/programming/node_apps/pies_fe/src/components/PieController.js",["96","97"],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/PieControls.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/components/PrivateRoute.js",[],"/home/geodevadmin/programming/node_apps/pies_fe/src/store.js",[],{"ruleId":"98","replacedBy":"99"},{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","severity":1,"mess